{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "/Serving/build_server/core/general-server/serving",
      "args": [
          "-enable_model_toolkit",
          "-inferservice_path", "workdir_8080",
          "-inferservice_file", "infer_service.prototxt",
          "-max_concurrency", "0",
          "-num_threads", "4",
          "-port", "8080",
          "-precision", "fp32",
          "-use_calib=False",
          "-reload_interval_s", "10",
          "-resource_path", "workdir_8080",
          "-resource_file", "resource.prototxt",
          "-workflow_path", "workdir_8080",
          "-workflow_file", "workflow.prototxt",
          "-bthread_concurrency", "4",
          "-max_body_size", "536870912"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/examples/C++/fit_a_line",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}